{
  "expo": {
    "name": "Bantu_Health",
    "slug": "bantuhealth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "bantuhealth",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.pixelelites.bantuhealth",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby medical facilities.",
        "NSCameraUsageDescription": "Camera access is required for video consultations",
        "NSMicrophoneUsageDescription": "Microphone access is required for video consultations",
        "NSPhotoLibraryUsageDescription": "Photo library access is required to upload images."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyAsc-PWMl_MI5iSNk9Jt61afWlZLFQ5Dmo"
      }
    },
    "androidNavigationBar": {
      "visible": "false"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "translucent": true,
      "barStyle": "light-content"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "newArchEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAsc-PWMl_MI5iSNk9Jt61afWlZLFQ5Dmo"
        }
      },
      "package": "com.pixelelites.bantuhealth",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "84da1bb7-b78b-4a92-bcf3-d54f0bc0a343"
      }
    }
  }
}
